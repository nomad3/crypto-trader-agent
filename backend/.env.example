BINANCE_API_KEY=YOUR_BINANCE_API_KEY
BINANCE_SECRET_KEY=YOUR_BINANCE_SECRET_KEY
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# --- Database ---
# Select DB_TYPE: "sqlite" or "postgres"
DB_TYPE=postgres

# Option 1: Define full URL (overrides individual components if DB_TYPE=postgres)
# DATABASE_URL=postgresql://user:password@db:5432/trading_db
DATABASE_URL=

# Option 2: Define individual components (used if DATABASE_URL is empty and DB_TYPE=postgres)
POSTGRES_HOST=db
POSTGRES_PORT=5432
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=trading_db

# Option 3: For SQLite (used if DB_TYPE=sqlite and DATABASE_URL is empty)
# Default path relative to backend dir: sqlite:///./backend/trading_agents.db
